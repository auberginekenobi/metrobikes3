package bikescheme;

public class User {
	
	//declare fields here
	//private String name;
	private String keyID;
	private boolean bikestat;
	private int curNorth;
	private int curEast;
	private Biketime curTime;
	//mke into dictionary? need to add trip things
	
	
	public User (String keyID) {
		//this.name = name;
		this.keyID = keyID;
		bikestat = false;
		curNorth = 0;
		curEast= 0;
		curTime= new Biketime (0, 0, 0);
	}
	
	//public String getName(){return name;}
	public String getUID() {return keyID;}
	public boolean getHasBike(){return bikestat;}
	public void setHasBike(boolean val) {bikestat = val;}

}
